{
  "name": "@artesoft/timeout-scheduler",
  "version": "1.3.1",
  "description": "A performance-oriented, freeze-proof scheduler that provides a priority-based task system to prevent UI blocking.",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minchopm/timeout-scheduler.git"
  },
  "keywords": [
    "setTimeout",
    "performance",
    "scheduler",
    "requestAnimationFrame",
    "requestIdleCallback",
    "priority",
    "cooperative",
    "freeze-proof",
    "adaptive",
    "dynamic",
    "angular",
    "react",
    "vue"
  ],
  "author": {
    "name": "Artesoft Inc.",
    "url": "https://www.arte-soft.com"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Mincho Milev",
      "email": "mincho.milev@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/minchopm/timeout-scheduler/issues"
  },
  "homepage": "https://github.com/minchopm/timeout-scheduler#readme",
  "dependencies": {
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
